{
  "name": "ui-scaffolding",
  "version": "1.0.0",
  "description": "Scaffolding for building React JS apps",
  "engines": {
    "node": ">=8.x"
  },
  "light": "dist/modern/index.html",
  "heavy": "dist/legacy/index.html",
  "targets": {
    "light": {
      "engines": {
        "browsers": "> 0.5%"
      },
      "distDir": ".build/modern"
    },
    "heavy": {
      "engines": {
        "browsers": "ie 11"
      },
      "distDir": ".build/legacy"
    }
  },
  "includeNodeModules": false,
  "outputFormat": "global",
  "isLibrary": false,
  "private": true,
  "browserslist": ["> 0.5%", "not ie 11"],
  "scripts": {
    "browsers": "npx browserslist",
    "clean": "rm -rf .build",
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venikanya/react-template.git"
  },
  "author": "venikanya",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/venikanya/react-template/issues"
  },
  "homepage": "https://github.com/venikanya/react-template#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "jest": "^26.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  }
}
